{
  "expo": {
    "name": "Travel See",
    "slug": "Travel See",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "Travel See",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.observerdev.travel-see",
      "config": {
        "MGLMapboxAccessToken": "pk.eyJ1Ijoib2JzZXJ2ZXJkZXYiLCJhIjoiY2xzeWIxN28yMGJxNzJscHA5cTlkY2hmcSJ9.f5saPByL2ncLa5q26001ow"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access camera.",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.observerdev.travelSee",
      "config": {
        "googleMaps": {
          "apiKey": "pk.eyJ1Ijoib2JzZXJ2ZXJkZXYiLCJhIjoiY2xzeWIxN28yMGJxNzJscHA5cTlkY2hmcSJ9.f5saPByL2ncLa5q26001ow"
        }
      },
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentmetroget": "13.0"
          }
        }
      ],
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/38ef0ffc-046c-432f-9c2a-02c770dbc84c"
    }
  }
}
